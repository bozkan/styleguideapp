<!DOCTYPE html>
<html>
  <head>
    <title>Styleguideapp</title>
    <%= csrf_meta_tags %>
    <%= csp_meta_tag %>

    <%= stylesheet_link_tag    'application', media: 'all', 'data-turbolinks-track': 'reload' %>
    <%= javascript_include_tag 'application', 'data-turbolinks-track': 'reload' %>
    <link rel="stylesheet" href="https://use.typekit.net/urr6rlc.css">
  </head>

  <body>
    <div class="menu">
      <%= render 'partials/brand_nav' %>
    </div>
    <div class="main">
      <div class="main-inner">
        <%= render 'partials/notice' %>
        <%= yield %>
        <img src="https://d28lxayvntpz3s.cloudfront.net/site-assets/logos/sg-logo.png" style="max-width:150px;position:absolute;bottom:20px;left:20px;">
      </div>
    </div>

  </body>
</html>
